/* ----------------- Bottom Bar ----------------- */
/*Status bar (inferior)*/
#bottomBar { background-color: #2A3C57 }
#bottomBar QLabel { font-size: 11px; color: #f8f8f2; padding-left: 10px; padding-right: 10px; padding-bottom: 2px; }


/* ----------------- QTableWidget ----------------- */
QTableWidget {	
	background-color: transparent;
/*	padding: 10px;*/
	border-radius: 5px;
	gridline-color: #6C99DD;
    outline: none;
}
QTableWidget::item{
	border-color: tranparent;
	padding-left: 5px;
	padding-right: 5px;
	gridline-color: transparent;
}

QTableWidget::item:selected{
	background-color:#6C99DD; /*color al seleccionar el cuadro*/
    color: white;/*color de la letra en el cuadro*/
}

QHeaderView::section{
	background-color: white;
	border: none;
	border-style: none;
}

/*
QTableWidget::horizontalHeader {	
	background-color: #6272a4;
}
*/

QHeaderView::section:horizontal
{
    border: 1px solid #1F54A3;
	background-color: #1F54A3;
/*	padding: 3px;*/
	border-top-left-radius: 7px;
    border-top-right-radius: 7px;
    color: white;
}

QHeaderView::section:vertical
{
    border: 1px solid #1F54A3;
	border-top-left-radius: 7px;
    border-bottom-left-radius: 7px;
	padding-left:4px;
}




/* ----------------- LineEdit  ----------------- */
QLineEdit {
	border-radius: 5px;
	border: 2px solid #1F54A3;
	padding-left: 10px;
	selection-color: white;
	selection-background-color: #2A3C57;
    color: black;
}
QLineEdit:hover {
	border: 2px solid rgb(98, 114, 164);
}
QLineEdit:focus {
	border: 2px solid #2A3C57;
}




/* ----------------- ScrollBars ----------------- */
/*tamaño y bordes*/
QScrollBar:horizontal {
    border: none;
    background: #6272a4;
    height: 8px;
    margin: 0px 21px 0 21px;
	border-radius: 0px;
}
/*la linea*/
QScrollBar::handle:horizontal {
    background: #296FD6;
    min-width: 25px;

}
/*bordecito inferior derecho*/
QScrollBar::add-line:horizontal {
    border: none;
    background: #102D57;
    width: 20px;
	border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
/*bordecito inferior izquierdo*/
QScrollBar::sub-line:horizontal {
    border: none;
    background: #102D57;
    width: 20px;
	border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

/*tamaño y bordes*/
 QScrollBar:vertical {
	border: none;
    background-color: #6272a4;
    width: 8px;
    margin: 21px 0 21px 0;
	border-radius: 0px;
 }
/*la linea*/
 QScrollBar::handle:vertical {	
	background: #296FD6;
    min-height: 25px;

 }
/*bordecito lateral inferior*/
 QScrollBar::add-line:vertical {
     border: none;
    background: #102D57;
     height: 20px;
	border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }
/*bordecito lateral superior*/
 QScrollBar::sub-line:vertical {
	border: none;
    background: #102D57;
     height: 20px;
	border-top-left-radius: 4px;
    border-top-right-radius: 4px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }

/* ----------------- RadioButton ----------------- */
QRadioButton::indicator {
    border: 3px solid #102D57;
	width: 12px;
	height: 12px;
	border-radius: 9px;
    background: #102D57;
}

QRadioButton::indicator:hover {
    border: 3px solid #1F54A3;
}

QRadioButton::indicator:checked {
    background: 3px solid #296FD6;
	border: 3px solid #296FD6;	
}
/* ----------------- QComboBox ----------------- */
QComboBox{
	background-color: white;
	border-radius: 5px;
	border: 2px solid #1F54A3;
    color: black;
}
QComboBox:hover{
	border: 2px solid #7284b9;
}
QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px; 
	border-left-width: 3px;
	border-left-color: white;
	border-left-style: solid;
	border-top-right-radius: 3px;
	background-image:url(:/icons/images/icons/cil-arrow-bottom.png);
	border-bottom-right-radius: 3px;	
	background-position: center;
	background-repeat: no-reperat;
 }

QComboBox QAbstractItemView {
	color: white;	
	background-color: #1F54A3;
	
	selection-background-color: #1F54A3;
}




